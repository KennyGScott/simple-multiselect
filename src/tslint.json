{
  "extends": "../tslint.json",
  "rules": {
    "one-line": false,
    "no-trailing-whitespace": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": false,
    "triple-equals": false,
    "curly": false,
    "no-string-literal": false,
    "semicolon": false
  }
}
